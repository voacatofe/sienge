# Configurações da API Sienge
SIENGE_SUBDOMAIN=seu-subdominio
SIENGE_USERNAME=seu-usuario-api
SIENGE_PASSWORD=sua-senha-api

# Configurações do Banco de Dados PostgreSQL
DATABASE_URL=postgresql://sienge_app:senha_forte@localhost:5432/sienge_data

# Configurações da Aplicação
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXTAUTH_SECRET=seu-secret-super-seguro-aqui
NEXTAUTH_URL=http://localhost:3000

# Configurações de Sincronização
SYNC_SCHEDULE=0 2 * * *
SYNC_BATCH_SIZE=200
SYNC_MAX_RETRIES=3

# Configurações de Log
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Configurações de Segurança
BCRYPT_ROUNDS=12
JWT_SECRET=seu-jwt-secret-aqui

# Configurações do Docker
POSTGRES_DB=sienge_data
POSTGRES_USER=sienge_app
POSTGRES_PASSWORD=senha_forte
POSTGRES_HOST=db
POSTGRES_PORT=5432