{
  "name": "sienge-data-sync",
  "version": "1.0.0",
  "description": "Aplicação containerizada para sincronização de dados da API Sienge com PostgreSQL",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "sync:daily": "node scripts/daily-sync.js",
    "sync:dependencies": "ts-node scripts/sync-with-dependencies.ts",
    "sync:test-errors": "ts-node scripts/test-sync-with-errors.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "sienge",
    "api",
    "postgresql",
    "docker",
    "nextjs",
    "data-sync"
  ],
  "author": "Darla",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^24.5.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.16.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.1",
    "axios-retry": "^4.5.0",
    "bcryptjs": "^3.0.2",
    "bottleneck": "^2.19.5",
    "dotenv": "^17.2.2",
    "next": "^14.0.0",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prisma": "^6.16.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.62.0",
    "tailwindcss": "^3.4.17",
    "zod": "^4.1.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
