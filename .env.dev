# Configurações da API Sienge (Desenvolvimento)
SIENGE_SUBDOMAIN=demo
SIENGE_USERNAME=demo_user
SIENGE_PASSWORD=demo_password

# Configurações do Banco de Dados PostgreSQL (Desenvolvimento)
POSTGRES_DB=sienge_dev
POSTGRES_USER=sienge_dev
POSTGRES_PASSWORD=dev_password
POSTGRES_HOST=db
POSTGRES_HOST_EXTERNAL=localhost
POSTGRES_PORT=5432

# URL de Conexão do Prisma (para desenvolvimento)
DATABASE_URL="postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}?schema=public"

# Configurações da Aplicação (Desenvolvimento)
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXTAUTH_SECRET=dev-secret-key-not-for-production
NEXTAUTH_URL=http://localhost:3000

# Configurações de Portas (Desenvolvimento)
APP_PORT_EXTERNAL=3000
APP_PORT_INTERNAL=3000
DB_PORT_EXTERNAL=5433
DB_PORT_INTERNAL=5432

# URL Externa da Base de Dados (para ferramentas BI)
# Esta URL será construída dinamicamente baseada nas variáveis acima
# Formato: postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@localhost:${DB_PORT_EXTERNAL}/${POSTGRES_DB}

# Configurações de Sincronização (Desenvolvimento)
SYNC_SCHEDULE=0 2 * * *
SYNC_BATCH_SIZE=50
SYNC_MAX_RETRIES=3

# Configurações de Log (Desenvolvimento)
LOG_LEVEL=debug
LOG_FILE=logs/app.log

# Configurações de Segurança (Desenvolvimento)
BCRYPT_ROUNDS=4
JWT_SECRET=dev-jwt-secret-not-for-production

# Configurações específicas para desenvolvimento
NODE_ENV=development

# ===========================================
# CONFIGURAÇÕES DO PGADMIN (DESENVOLVIMENTO)
# ===========================================
PGADMIN_EMAIL=darlan@catofe.com.br
PGADMIN_PASSWORD=admin123
PGADMIN_PORT_EXTERNAL=8080
